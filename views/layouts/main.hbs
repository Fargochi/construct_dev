<html>
    <head>
        <title>Конструктор приложений</title>
		<link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
		<script src="/js/jquery-2.2.4.min.js"></script>
		<script src="/js/jquery-ui.min.js"></script>
		<script type="text/javascript" src="https://swisnl.github.io/jQuery-contextMenu/dist/jquery.contextMenu.min.js"></script>
      <link rel="stylesheet" type="text/css" href="https://swisnl.github.io/jQuery-contextMenu/dist/jquery.contextMenu.min.css">
		 <script src='/js/main.js'></script>
        <link rel='stylesheet' href='/css/main.css'>
		<script>
		document.oncontextmenu=new Function("return false;")
		</script>
		
		
		
		 <style type="text/css">


		.context-menu-one {
		padding: 10px;
		}

		#res {
		margin-top: 10px;
		}

		</style>
		<script type="text/javascript">
		$(window).load(function(){
			$.contextMenu({
				selector: '.context-menu-one',
				items: {
					open: {name: 'Открыть', icon: 'open'},
				save:  {name: 'Сохранить', icon: 'save'},
				sep1: '---------',
				quit: {name: 'Выйти', icon: 'quit'}
				},
			callback: function(key, options) {
					$('#res').html('Выбрана команда: <strong>' + key + '</strong>');
					}
			});
		});
		</script>

    </head>
    <body oncontextmenu="return false" bgcolor="e7e7e7">
        {{{body}}}
        <img src="/img/kot.JPG" id="kot"></img>
    </body>
</html>